alias cp='cp -i'
alias groupsp='groups | sed "s/ /\n    /g"'
alias l='ls -CF'
alias ll='ls -l'
alias lla='ls -la'
alias mv='mv -i'
alias r='reset'

# git aliases
alias config='/usr/bin/git --git-dir=$HOME/.cfg/ --work-tree=$HOME'

# tmux aliases
alias tko='tmux kill-session -t'

# HUBzero aliases
alias hub='. ~/.bin/hub'
